doctype html
head
    title Ac-News
    style 
        include ../static/style.css
    script(src='../static/index.js')
body
    nav#navbar 
        ul 
            li 
                a(href='#ac-news') Ac-News
            li 
                a.active(href='#home') Home
            li 
                a(href='#news') News
            li 
                a(href='#about') About
            li 
                a(href='#contact') Contact

    section#sec1
        ol 
            each val in data
                li <b>#{val.title}</b> (Cateogry :-#{val.category})
                    <br>  
                    p #{val.description} 
                    a.link(href=`${val.url}`) Read More...
                <br>

    section.pagination
        a(href='') &laquo;
        a.active(href='') 1
        a(href='#') 2
        a(href='') 3
        a(href='') 4
        a(href='') 5
        a(href='') 6
        a(href='') &raquo;
